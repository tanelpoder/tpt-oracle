AND e.first_name        = 'William'
AND e.last_name         = 'Smith'
AND prod.product_name   = 'Mobile Web Phone'
AND c.cust_first_name   = 'Gena'
AND c.cust_last_name    = 'Harris'
GROUP BY
    d.department_name
  , e.first_name
  , e.last_name
  , prod.product_name
  , c.cust_first_name
  , c.cust_last_name
